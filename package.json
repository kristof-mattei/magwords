    "prettier-plugin-toml": "2.0.6",
    "sass-embedded": "1.93.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0",
    "vite": "7.2.4",
    "vite-plugin-checker": "0.11.0",
    "vite-plugin-svgr": "4.5.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.12"
  }
